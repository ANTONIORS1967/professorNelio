package ws_eclipse;



public class Main {
	
	public static void main(String[] args) {
		
		double b = 2;
		double a = 1;
		double c = 3;
		double  delta;
		double x1;
		double x2;
		
		 
		delta = Math.pow(b, 2.0) - 4*a*c;
		
		x1 = (-b + Math.sqrt(delta)) / (2.0 * a);
		x2 = (-b - Math.sqrt(delta)) / (2.0 * a);
	}
}