package entities;

import entities.enums.Color;

public abstract class Shap {
	private Color color;
	
	public Shap() {
		super();
	}

	public Shap(Color color) {
		super();
		this.color = color;
	}


	public Color getColor() {
		return color;
	}

	public void setColor(Color color) {
		this.color = color;
	}
	public abstract double area();
	
}
