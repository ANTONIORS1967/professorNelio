package application;

import java.text.ParseException;
import java.text.SimpleDateFormat;
import java.util.Date;

public class Program {



	public static void main(String[] args) throws ParseException {
				

		SimpleDateFormat asdf1 = new SimpleDateFormat("dd/MM/yyy");
		SimpleDateFormat asdf2 = new SimpleDateFormat("dd/MM/yyy HH:mm:ss");
		
		Date y1 = asdf1.parse("25/06/2018");
		Date y2 = asdf2.parse("25/06/2018 15:42:07");
		
		
		System.out.println("y1 = " +asdf1.format(y1));
		System.out.println("y2 = " +asdf2.format(y2));
		
		
		
		
		
			}

	private static void parse(String string) {
		// TODO Auto-generated method stub
		
	}	
		
	}


