package application;

import java.util.List;
import java.util.ArrayList;

public class Program {

	public static void main(String[] args) {

		List<String> list = new ArrayList<>();
		list.add("Maria");
		list.add("Alex");
		list.add("Bob");
		
		
		System.out.println(list.size());
		for(String x : list) {
			System.out.println(x);
		}
	}

}
